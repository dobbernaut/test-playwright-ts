{
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "out-tsc",
    "module": "commonjs",
    "target": "es2018",
    "typeRoots": [
      "node_modules/@types",
      "typings"
    ],
    "types": [
      "node",
      "playwright",
      "@types/chai",
      "@types/mocha",
      "global"
    ],
    "paths": {
      "@enums/*": ["./test/enums/*"],
      "@pages/*": ["./test/pages/*"],
      "@services/*": ["./test/services/*"],
      "@interfaces/*": ["test/interfaces/*"],
      "@test/*": ["./test/*"],
    }
  }
}
